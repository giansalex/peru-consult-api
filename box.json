{
    "alias": "composer.phar",
    "chmod": "0755",
    "directories": ["src", "templates"],
    "files": [
        "public/index.php",
        "public/swagger.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["tests"],
            "in": "vendor"
        }
    ],
    "compression" : "GZ",
    "main" : "public/index.php",
    "not-found" : "public/index.php",
    "output": "consult.phar",
    "stub": true,
    "web": false,
    "banner": "Peru Consult API",
    "shebang": false
}